{
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --no-cache",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "omit-ts",
  "author": "Jeff Reiner",
  "module": "dist/omit-ts.esm.js",
  "size-limit": [
    {
      "path": "dist/omit-ts.cjs.production.min.js",
      "limit": "50 B"
    },
    {
      "path": "dist/omit-ts.esm.js",
      "limit": "50 B"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.3",
    "husky": "^7.0.1",
    "size-limit": "^5.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "description": "Absolutely teeny tiny (less than 50 Bytes) Typescript omit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mirshko/omit-ts.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/mirshko/omit-ts/issues"
  },
  "homepage": "https://github.com/mirshko/omit-ts#readme"
}
