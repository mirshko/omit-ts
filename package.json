{
  "name": "omit-ts",
  "author": "Jeff Reiner",
  "description": "Absolutely teeny tiny (less than 50 Bytes) Typescript omit",
  "version": "1.0.1",
  "license": "MIT",
  "type": "module",
  "source": "src/omit.ts",
  "exports": {
    "require": "./dist/omit.cjs",
    "default": "./dist/omit.modern.js"
  },
  "main": "./dist/omit.cjs",
  "module": "./dist/omit.umd.js",
  "unpkg": "./dist/omit.umd.js",
  "types": "./dist/omit.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "microbundle watch",
    "build": "microbundle",
    "prepare": "yarn run build"
  },
  "devDependencies": {
    "microbundle": "^0.15.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mirshko/omit-ts.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/mirshko/omit-ts/issues"
  },
  "homepage": "https://github.com/mirshko/omit-ts#readme"
}
